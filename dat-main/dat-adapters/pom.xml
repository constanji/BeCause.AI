<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <!-- 当前模块的父模块，groupId 和 artifactId 是父模块的坐标 -->
    <parent>
        <groupId>cn.hexinfo</groupId>
        <artifactId>dat-parent</artifactId>
        <version>${revision}</version>
    </parent>

    <artifactId>dat-adapters</artifactId>
    <packaging>pom</packaging>   <!-- 父模块的打包类型为 pom -->
    <name>DAT : Adapters :</name>
    
    <!-- 当前模块的子模块 -->
    <modules>
        <module>dat-adapter-mysql</module>
        <module>dat-adapter-postgresql</module>
        <module>dat-adapter-oracle</module>
        <module>dat-adapter-duckdb</module>
    </modules>

</project>
